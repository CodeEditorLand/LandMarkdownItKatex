		"watch": "tsc -p . -watch"
	},
	"dependencies": {
		"katex": "0.16.9"
	},
	"devDependencies": {
		"@types/katex": "0.16.7",
		"@types/markdown-it": "13.0.7",
		"markdown-it": "14.1.0",
		"markdown-it-testgen": "0.1.6",
		"tape": "5.7.5",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
